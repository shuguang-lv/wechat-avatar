			"style": {
				"enablePullDownRefresh": false,
				"disableScroll": true
			}
		},
		{
			"path": "pages/main/index",
			"style": {
				"enablePullDownRefresh": false,
				"disableScroll": true
			}
		},
		{
			"path": "pages/save-success/index",
